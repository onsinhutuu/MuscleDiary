<div class="row justify-content-center">
	<% @users.each do |user| %>
	  <table class="table table-striped table-bordered">
	      <tr>
		      <td class="follow-show">
		        <%= link_to "#{ user.name }のページへいく", user_path(user), class: "btn btn-light" %>
		      </td>
		      <td class="follow-show">
		         <%= link_to "マイページへ", user_path(user.id), class: "btn btn-light" %>
		      </td>
				</tr>
		</table>
	<% end %>
</div>
